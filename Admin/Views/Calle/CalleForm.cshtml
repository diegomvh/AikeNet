<div class="modal hide fade" id="CalleModal" tabindex="-1" role="dialog" aria-labelledby="CalleModalLabel" aria-hidden="true" >
  <div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">X</button>
    <h3 id="CalleModalLabel">Calle</h3>
  </div>
  <div class="modal-body">
    <form id="CalleForm" class="form-horizontal">
    	<input id="inputCID" type="hidden" name="cId" value="@Model.Calle.id" />
        <div class="control-group">
            <label class="control-label" for="inputNombre">Nombre</label>
            <div class="controls">
                <input id="inputNombre" type="text" name="nombre" placeholder="Nombre" />
            </div>
        </div>
    </form>
  </div>
  <div id="formErrors" class="alert alert-block hide">
  </div>
  <div class="modal-footer">
    <button class="btn" data-dismiss="modal" aria-hidden="true">Cancelar</button>
    <button id="buttonGuardarCalle" class="btn btn-primary">Guardar</button>
  </div>
</div>
