@model IEnumerable<CoreSQL.Entities.AlturasCalle>

@{
    ViewBag.Title = "Alturas";
}

@if (Model.Count() != 0)
{
    <table class="table table-bordered">
	    <thead>
	    <tr>
		    <th>Calle</th>
		    <th>Desde</th>
		    <th>Hasta</th>
		    <th>Vereda</th>
		    <th>Zona</th>
	    </tr>
	    </thead>
	    <tbody>
		    @Html.DisplayForModel("CalleTableRow")
	    </tbody>
    </table>
}
else { 
    <h2>sin resultados</h2>
}